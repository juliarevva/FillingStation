import java.util.Scanner;

public class Main {
    public static void main(String[] args) {
        int value = new Scanner(System.in).nextInt();
        for (int a = 1; a <= value; a = a + 1) {
            if (value % a != 0) {
                continue;
            }

            for (int b = value / a; ; ) {
                System.out.println(a + "*" + b);
                break;
            }
        }

    }
}
